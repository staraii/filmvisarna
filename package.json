{
  "name": "filmvisarna",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "backend:compile": "cd backend && tsc",
    "backend:watch": "cd backend && tsc --watch",
    "backend:run": "nodemon --env-file=.env backend/backendDist/index.js",
    "start": "npm run backend:compile && concurrently \"npm run backend:watch\" \"npm run backend:run\" \"npm run dev\"",
    "dev": "cd frontend && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.3",
    "connect-mysql": "^4.0.0",
    "connect-session-knex": "^5.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.1",
    "knex": "^3.1.0",
    "mysql2": "^3.11.3",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/bcrypt": "^5.0.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/express-mysql-session": "^3.0.6",
    "@types/express-session": "^1.18.0",
    "@types/knex": "^0.15.2",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.0.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "nodemon": "^3.1.7",
    "sass": "^1.79.2",
    "sass-embedded": "^1.79.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
