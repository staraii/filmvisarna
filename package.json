{
  "name": "filmvisarna",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server:compile": "cd server && tsc",
    "server:watch": "cd server && tsc --watch",
    "server:run": "nodemon --env-file=.env server/serverDist/index.js",
    "start": "npm run server:compile && concurrently \"npm run server:watch\" \"npm run server:run\" \"npm run dev\"",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "express": "^4.21.0",
    "mysql2": "^3.11.3",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.0.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "nodemon": "^3.1.7",
    "sass": "^1.79.2",
    "sass-embedded": "^1.79.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
